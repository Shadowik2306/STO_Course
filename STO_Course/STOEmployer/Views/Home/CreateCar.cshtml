@{
	ViewData["Title"] = "CreateCar";
}
<div class="text-center">
	<h2 class="display-4">Создание машины</h2>
</div>
<form method="post">
	<div class="row">
		<div class="col-4">Марка:</div>
		<div class="col-8"><input type="text" name="title" id="title" /></div>
	</div>
	<div class="row">
		<div class="col-4">Модель:</div>
		<div class="col-8"><input type="number" name="price" id="price" /></div>
	</div>
	<div class="row">
		<div class="col-4">VIN:</div>
		<div class="col-8"><input type="number" name="price" id="price" /></div>
	</div>
	<div class="row">
		<div class="col-8"></div>
		<div class="col-4">
			<input type="submit" value="Создание" class="btn btn-primary" />
		</div>
	</div>
</form>